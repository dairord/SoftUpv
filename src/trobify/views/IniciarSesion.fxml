<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="650.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="trobify.controlador.IniciarSesionController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="346.0">
         <items>
            <Button mnemonicParsing="false" onAction="#atras" text="Atras" />
         </items>
      </ToolBar>
      <HBox alignment="CENTER" prefHeight="42.0" prefWidth="331.0">
         <children>
            <Label fx:id="mensajeError" prefHeight="33.0" prefWidth="235.0" textFill="#f50000">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="84.0" prefWidth="331.0">
         <children>
            <Label prefHeight="25.0" prefWidth="143.0" text="Nombre de usuario" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="39.0" prefWidth="331.0">
         <children>
            <TextField fx:id="nombre" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="63.0" prefWidth="331.0">
         <children>
            <Label prefHeight="17.0" prefWidth="147.0" text="Contraseña" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="55.0" prefWidth="331.0">
         <children>
            <PasswordField fx:id="contraseña" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="aceptarBoton" mnemonicParsing="false" onAction="#aceptar" text="Aceptar" />
         </children>
      </HBox>
   </children>
</VBox>
